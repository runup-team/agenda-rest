{
  "name": "agenda-rest",
  "version": "1.1.0",
  "description": "Scheduling as a Service",
  "main": "./dist/index.js",
  "jsnext:main": "./src/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "lint": "xo",
    "dev": "webpack --mode development",
    "build": "webpack --mode production",
    "test": "ava dist/test.js && npm run lint",
    "start": "npm run dev && node cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/agenda/agenda-rest.git"
  },
  "files": [
    "cli.js",
    "settings.js",
    "dist"
  ],
  "keywords": [
    "schedule",
    "scheduling",
    "scheduler",
    "webservice",
    "microservice",
    "restful",
    "rest",
    "agenda"
  ],
  "author": "Keyvan Mir Mohammad Sadeghi <keyvan.m.sadeghi@gmail.com>",
  "license": "MIT",
  "bin": {
    "agenda-rest": "cli.js"
  },
  "bugs": {
    "url": "https://github.com/agenda/agenda-rest/issues"
  },
  "homepage": "https://github.com/agenda/agenda-rest#README",
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.3.3",
    "@babel/preset-env": "7.3.1",
    "@babel/register": "7.0.0",
    "ava": "1.2.1",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "eslint": "5.14.1",
    "eslint-loader": "2.1.2",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-node": "8.0.1",
    "webpack": "4.29.5",
    "webpack-cli": "3.2.3",
    "xo": "0.23.0"
  },
  "dependencies": {
    "agenda": "^2.0.2",
    "babel-runtime": "^6.26.0",
    "commander": "^2.18.0",
    "koa": "^2.5.3",
    "koa-bodyparser": "^4.2.1",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "pythonic": "^1.0.1",
    "regenerator-runtime": "^0.13.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "supertest": "^3.3.0"
  },
  "engines": {
    "node": ">=8"
  },
  "xo": {
    "space": 2,
    "rules": {
      "promise/param-names": 0,
      "promise/prefer-await-to-then": 0,
      "max-params": [
        "error",
        5
      ],
      "max-nested-callbacks": [
        "error",
        5
      ]
    },
    "envs": [
      "node"
    ]
  }
}
